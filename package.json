{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 5328",
    "next-dev": "next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prodFE": "pnpm run build && pnpm run start",
    "prodBE": "pip3 install -r requirements.txt && waitress-serve --port=5328 --call api.index:create_app",
    "prod": "concurrently \"pnpm run prodFE\" \"pnpm run flask-dev\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "binary-parser": "^2.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "headlessui": "^0.0.0",
    "heroicons": "^2.2.0",
    "lucide-react": "^0.453.0",
    "next": "13.4.3",
    "next-themes": "^0.3.0",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.5",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.0.4"
  }
